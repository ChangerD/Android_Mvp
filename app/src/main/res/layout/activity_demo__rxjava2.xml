<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="20px"
    tools:context="com.changer.www.mvp.demo.rxjava2.Demo_Rxjava2_Activity">

    <Button
        android:id="@+id/arx2_btn_1"
        android:layout_width="match_parent"
        android:layout_height="150px"
        android:text="观察者和被观察者"/>

    <Button
        android:id="@+id/arx2_btn_2"
        android:layout_width="match_parent"
        android:layout_height="150px"
        android:text="发射和阻断"/>

    <Button
        android:id="@+id/arx2_btn_3"
        android:layout_width="match_parent"
        android:layout_height="150px"
        android:text="线程切换"/>

    <Button
        android:id="@+id/arx2_btn_4"
        android:layout_width="match_parent"
        android:layout_height="150px"
        android:text="操作符-Map"/>

    <Button
        android:id="@+id/arx2_btn_5"
        android:layout_width="match_parent"
        android:layout_height="150px"
        android:text="操作符-FlatMap-ConcatMap"/>

    <Button
        android:id="@+id/arx2_btn_6"
        android:layout_width="match_parent"
        android:layout_height="150px"
        android:text="操作符-Zip"/>

    <Button
        android:id="@+id/arx2_btn_7"
        android:layout_width="match_parent"
        android:layout_height="150px"
        android:text="背压处理-Backpressure"/>

    <Button
        android:layout_width="match_parent"
        android:layout_height="150px"
        android:text="网络-retrofit"/>

    <Button
        android:layout_width="match_parent"
        android:layout_height="150px"
        android:text="数据库"/>

</com.zhy.autolayout.AutoLinearLayout>
