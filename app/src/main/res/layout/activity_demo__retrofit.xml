<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="20px"
    tools:context="com.changer.www.mvp.demo.retrofit.Demo_Retrofit_Activity">

    <Button
        android:id="@+id/rtf_btn_1"
        android:layout_width="match_parent"
        android:layout_height="150px"
        android:text="发送请求" />

    <Button
        android:id="@+id/rtf_btn_2"
        android:layout_width="match_parent"
        android:layout_height="150px"
        android:text="上传单文件" />

    <Button
        android:id="@+id/rtf_btn_3"
        android:layout_width="match_parent"
        android:layout_height="150px"
        android:text="上传多文件" />

    <Button
        android:id="@+id/rtf_btn_4"
        android:layout_width="match_parent"
        android:layout_height="150px"
        android:text="上传单文件带参" />

    <Button
        android:id="@+id/rtf_btn_5"
        android:layout_width="match_parent"
        android:layout_height="150px"
        android:text="上传多文件带参" />

</com.zhy.autolayout.AutoLinearLayout>
